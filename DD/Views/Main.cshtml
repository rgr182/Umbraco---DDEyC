	<link href="~/Styles/bootstrap-5.1.3-dist/css/bootstrap.min.css" rel="stylesheet" />
	<link href="~/Styles/styles.css" rel="stylesheet" />
	<link href="~/Styles/sharedContentPageStyles.css" rel="stylesheet" />
