@using Umbraco.Cms.Web.Common.PublishedModels;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage<LandingPage>
@{
	Layout = "Main.cshtml";
}
<div>
	@await Html.PartialAsync("Partials/homePage/_navigation.cshtml")
</div>
<div class="jumbotron text-center">
	@await Html.PartialAsync("Partials/homePage/_dropBox.cshtml")
	<hr class="my-4">
	<div class="mt-3">
		@await Html.PartialAsync("Partials/homePage/_redirectButton")
	</div>
	<div class="mt-3">
		@await Html.PartialAsync("Partials/homePage/_imageCarousel", Model.ImageCarousel)
	</div>
	<div class="mt-3">
		@{
			@await Html.PartialAsync("Partials/blocklistItems/_embededVideo.cshtml", Model.EmbededVideo?.Content)
		}
	</div>
	<div class="mt-3">
		@await Html.PartialAsync("Partials/homePage/_headlineTitle")
		@await Html.PartialAsync("Partials/homePage/_newsCarousel", Model.NewsCarousel)
	</div>
	<div class="mt-3">
		@{
			var textAndImageBlocks = Model.RichTextAndImages;
			if (textAndImageBlocks != null)
			{
				foreach (var block in textAndImageBlocks)
				{
					@await Html.PartialAsync("Partials/_textAndImage", block.Content)
				}
			}
		}
	</div>
</div>
<div class="mt-3">
	@await Html.PartialAsync("Partials/homePage/_footerHeadlineTitle")
	@await Html.PartialAsync("Partials/homePage/_footer")
</div>
